<div class="content">
  <h2>Archivo de guías visuales editoriales</h2>
  <ul class="posts">
    <% @posts.each do |post| %>
    <li class="post">
      <%= post_status(post) %>
      <%= link_to(post.title, post_path(post)) %>
      - <%= link_to('editar', edit_admin_visual_guide_video_path(post)) %>
      - Código: <%= post.video_code %>
    </li>
    <% end %>
  </ul>

  <%= paginate @posts %>
</div>

